@model IEnumerable<KTI.Models.Tamu>

@{
    ViewBag.Title = "Index";
}

<h2>Daftar Tamu</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nama_Tamu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alamat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.No_Hp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ucapan_Selamat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_Tamu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alamat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.No_Hp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ucapan_Selamat)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Tamu })
        </td>
    </tr>
}

</table>
